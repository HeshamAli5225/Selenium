<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="nopeCommerce Regression Suite" parallel="tests" thread-count="4">
    <test name="Chrome Regression Test">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="tests.UserRegistrationParallelTest"/>
        </classes>
    </test>
    <test name="FireFox Regression Test">
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="tests.UserRegistrationParallelTest"/>
        </classes>
    </test>
    <test name="MicrosoftEdge Regression Test">
        <parameter name="browser" value="MicrosoftEdge"></parameter>
        <classes>
            <class name="tests.UserRegistrationParallelTest"/>
        </classes>
    </test>
</suite>